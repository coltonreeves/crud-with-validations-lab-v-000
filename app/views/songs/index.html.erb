
<div><%= link_to 'Create Song', new_song_path %></div>


<table>
  <thead>
    <tr>
      <th>Song ID</th>
      <th>Song name</th>
      <th>Artist Name</th>
      <th>Genre</th>
      <th>Released Yet?</th>
      <th>Year Released</th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.id %></td>
        <td><%= song.title %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.genre %></td>
        <td><%= song.released %></td>
        <td><%= song.release_year %></td>
        <td>
          <%= link_to 'View', song %>
          <%= link_to 'Edit', edit_song_path(song) %>
          <%= button_to 'Delete', song, method: :delete, data: { confirm: "Really?"} %>
        </td>
      </tr>
    <% end %>
  <tbody>
</table>
